{
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning",
        "Microsoft.AspNetCore.Mvc": "Warning",
        "Microsoft.AspNetCore.Components": "Warning",
        "Microsoft.AspNetCore.Server": "Warning",
        "Microsoft.EntityFrameworkCore": "Warning",
        "System": "Warning",
        "GrayMoon.App": "Verbose"
      }
    },
    "WriteTo": [
      { "Name": "Console" }
    ]
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.AspNetCore.Mvc": "Warning",
      "Microsoft.AspNetCore.Components": "Warning",
      "Microsoft.AspNetCore.Server": "Warning",
      "Microsoft.EntityFrameworkCore": "Warning",
      "System": "Warning",
      "GrayMoon.App": "Trace"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "DefaultConnection": "Data Source=db/graymoon.db"
  },
  "GitHub": {
    "PersonalAccessToken": "",
    "ApiBaseUrl": "https://api.github.com/"
  },
  "Workspace": {
    "MaxConcurrentGitOperations": 8,
    "PostCommitHookBaseUrl": "",
    "PostCommitHookPort": 8384
  },
  "Sync": {
    "MaxConcurrency": 16,
    "EnableDeduplication": true
  }
}
